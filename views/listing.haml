#leftPan
  %span.headline
    %b= !@dir.nil? ? "Cars on lot"  : "There's no car in the lot"
  - if !@dir.nil?    
    - @dir.each do |d|
      %p      
        %a{href: "/#{d[/\.\/[\S]+\/[\S]+\/[\S]+\/([\S]+)\//ix,1]}"}= d[/\.\/[\S]+\/[\S]+\/[\S]+\/([\S]+)\//ix,1]
  - else
    %p= "Empty"                    
